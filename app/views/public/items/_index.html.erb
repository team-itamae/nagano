 <div class="container">
  <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4">

    <% items.each do |item| %>
       <div class="col">
          <ul>
            <%= link_to "/items/#{item.id}" do %>
            <%= image_tag item.image %>
            <% end %>
          </ul>
          <ul><%= item.name %></ul>
          <ul><%= item.price.to_s(:dalimited) %></ul>
       </div>
    <% end %>

 </div>
</div>

